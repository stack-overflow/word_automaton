<Window x:Class="WordAutomationSharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBlock Visibility="Collapsed" x:Name="OutputTextBox" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="299" Width="208"/>
        <TreeView x:Name="OutputTreeView" Loaded="OutputTreeView_Loaded" HorizontalAlignment="Stretch" Height="Auto" Margin="12,12,150,50" VerticalAlignment="Stretch" Width="Auto"/>
        <TextBox Height="23" HorizontalAlignment="Stretch" Margin="12,0,12,12" Name="textBox1" VerticalAlignment="Bottom" Width="Auto" TextChanged="textBox1_TextChanged" />
        <ListBox Height="Auto" HorizontalAlignment="Right" Margin="0,12,12,50" Name="listBox1" VerticalAlignment="Stretch" Width="132" />
    </Grid>
</Window>
